{
  "name": "wayland-protocols",
  "author": "Timbits",
  "version": "0.1.3",
  "os": ["linux"],
  "cpu": ["x64", "arm64"],
  "main": "target/release/wayland-protocols.node",
  "napi": {
    "binaryName": "wayland-protocols",
    "targets": [
      "x86_64-unknown-linux-gnu",
      "aarch64-unknown-linux-gnu"
    ]
  },
  "files": [
	  "index.js",
	  "index.d.ts",
	  "prebuilds/**"
  ],
  "engines": {
    "node": ">= 12.22.0 < 13 || >= 14.17.0 < 15 || >= 15.12.0 < 16 || >= 16.0.0"
  },
  "scripts": {
    "build": "napi build --release && mv wayland-protocols.node target/release/wayland-protocols.node",
    "test": "npm run build && node test.js"
  },
  "devDependencies": {
    "@napi-rs/cli": "^3.2.0"
  }
}
